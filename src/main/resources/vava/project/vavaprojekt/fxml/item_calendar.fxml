<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<HBox fx:id="list_item" alignment="CENTER_LEFT" prefHeight="35.0" prefWidth="656.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vava.project.vavaprojekt.fxml.ItemCalendar">
   <children>
      <Text fx:id="item_time" strokeType="OUTSIDE" strokeWidth="0.0" text="00:00">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <HBox.margin>
            <Insets left="20.0" />
         </HBox.margin>
      </Text>
      <Separator orientation="VERTICAL" prefHeight="200.0">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Separator>
      <Text fx:id="item_name" strokeType="OUTSIDE" strokeWidth="0.0" text="Name of workout">
         <font>
            <Font size="14.0" />
         </font>
         <HBox.margin>
            <Insets left="20.0" right="40.0" />
         </HBox.margin>
      </Text>
      <ProgressBar fx:id="item_progress" prefHeight="20.0" prefWidth="142.0" progress="0.0" />
   </children>
</HBox>
